1st round picks of the NFL 2023 Draft
Collected by: Brandon Le
*/

CREATE TABLE picks(
	id INTEGER PRIMARY KEY,
	pick_number INTEGER,
	name TEXT,
	college TEXT,
	picked_by TEXT,
	position TEXT);

